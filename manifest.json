{
  "name": "jump to same link of tab",
  "description": "既に開いているタブの中にこれから開こうとしているリンクがあったらそのリンクに飛ぶ拡張機能",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [ "background.js"]
  },
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  }



}